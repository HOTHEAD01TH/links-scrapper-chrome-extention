{
  "manifest_version": 3,
  "name": "Link Saver",
  "version": "1.0.0",
  "description": "Saves all links from webpages",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "downloads",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Link Saver",
    "default_icon": "icon.png"  
  }
}